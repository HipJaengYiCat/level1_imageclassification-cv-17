{
    "seed": 42,
    "epochs": 20,
    "dataset": "MaskBaseDataset",
    "augmentation": "BaseAugmentation",
    "resize": [
        128,
        96
    ],
    "batch_size": 128,
    "valid_batch_size": 128,
    "model": "WideResNet101",
